{
    "name": "yiisoft/yii2-app-basic",
    "description": "Yii 2 Basic Project Template",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.0",
        "yiisoft/yii2": "^2.0",
        "yiisoft/yii2-bootstrap": "^2.0",
        "yiisoft/yii2-swiftmailer": "^2.0",
		"webvimark/module-user-management": "^1.0",
        "dmstr/yii2-adminlte-asset": "^2.3",
        "vova07/yii2-imperavi-widget": "^1.2",
        "kartik-v/yii2-widget-switchinput": "^1.3",
        "kartik-v/yii2-widget-fileinput": "^1.0",
        "kartik-v/yii2-date-range": "^1.6",
        "kartik-v/yii2-widget-activeform": "^1.4",
        "kartik-v/yii2-widget-select2": "@dev",
        "fxp/composer-asset-plugin": "^1.2.0",
        "wbraganca/yii2-videojs-widget": "~1.0",
        "yiisoft/yii2-imagine": "*",
        "wbraganca/yii2-selectivity": "*",
        "kartik-v/bootstrap-star-rating": "@dev"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "jamband/yii2-schemadump": "*"
    },
    "config": {
        "process-timeout": 1800,
	"fxp-asset": {
		"installer-paths": {
		    "npm-asset-library": "vendor/npm",
		    "bower-asset-library": "vendor/bower"
		},
		"vcs-driver-options": {
		    "github-no-api": true
		},
		"pattern-skip-version": "(-build|-patch)"
	    }
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    }
}
